<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Prescription Number Sequence -->
        <record id="seq_pharmacy_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">pharmacy.prescription</field>
            <field name="prefix">RX</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Insurance Claim Number Sequence -->
        <record id="seq_insurance_claim" model="ir.sequence">
            <field name="name">Insurance Claim Sequence</field>
            <field name="code">insurance.claim</field>
            <field name="prefix">CLM</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
